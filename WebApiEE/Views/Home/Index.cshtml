@{
    ViewBag.Title = "Student Register";
}

<script src="~/Scripts/knockout-3.2.0.js"></script>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Knockout/IndexViewModel.js"></script>

<div class="page-header">
    <h2 class="text-center">Student Registration</h2>
</div>

<div class="row">

    <div class="col-md-4">
        <div class="panel panel-info">
            <div class="panel-heading">
                <h2 class="panel-title">Register New Student</h2>
            </div>
            <div class="panel-body"
                 data-bind="with: registerStudentViewModel">
                @Html.Partial("_RegisterStudent")
            </div>
        </div>
    </div>

    <div class="col-md-8">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h2 class="panel-title">Registered Students</h2>
            </div>
            <div class="panel-body"
                 data-bind="with: studentListViewModel">
                @Html.Partial("_StudentList")
            </div>
        </div>
    </div>

</div>